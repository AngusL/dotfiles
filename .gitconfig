[user]
	name = Angus Lepper
	email = angus.lepper@gmail.com
[merge]
	tool = vimdiff
[alias]
	exec = "!exec "
	slog = "!git log --pretty=tformat:'%h%ad%an%s' --date=iso $1 | column -t -s '' | less -FX #"
	xl = "!git log --oneline --graph --all $1 #"
[init]
	templatedir = ~/.git_template
[diff]
	algorithm = patience
[color]
	ui = on
[push]
	default = upstream
[rerere]
	enabled = 1
